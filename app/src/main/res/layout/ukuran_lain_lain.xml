<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <TextView
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/header_form"
        android:textSize="@dimen/textsize"
        android:textColor="@color/White"
        android:text="UKURAN NH FUSE" />
    <TableLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/border"
        android:padding="5dp" >

        <TableRow
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" >
            <TableRow
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" >
                <TextView
                    android:layout_width="92dp"
                    android:background="@drawable/border"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:padding="15dip"
                    android:text="JURUSAN" />
                <TextView
                    android:layout_width="93dp"
                    android:background="@drawable/border"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:padding="15dip"
                    android:text="R" />
                <TextView
                    android:layout_width="93dp"
                    android:background="@drawable/border"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:padding="15dip"
                    android:text="S" />
                <TextView
                    android:layout_width="93dp"
                    android:background="@drawable/border"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:padding="15dip"
                    android:text="T" />
            </TableRow>

        </TableRow>
        <TableRow
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" >
            <TableRow
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" >
                <TextView
                    android:layout_width="92dp"
                    android:background="@drawable/border"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:padding="15dip"
                    android:text="A" />
                <EditText
                    android:id="@+id/a_r"
                    android:layout_width="93dp"
                    android:background="@drawable/border"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:padding="15dip" />
                <EditText
                    android:id="@+id/a_s"
                    android:layout_width="93dp"
                    android:background="@drawable/border"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:padding="15dip"/>
                <EditText
                    android:id="@+id/a_t"
                    android:layout_width="93dp"
                    android:background="@drawable/border"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:padding="15dip"/>
            </TableRow>

        </TableRow>
        <TableRow
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" >
            <TableRow
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" >
                <TextView
                    android:layout_width="92dp"
                    android:background="@drawable/border"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:padding="15dip"
                    android:text="B" />
                <EditText
                    android:id="@+id/b_r"
                    android:layout_width="93dp"
                    android:background="@drawable/border"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:padding="15dip" />
                <EditText
                    android:id="@+id/b_s"
                    android:layout_width="93dp"
                    android:background="@drawable/border"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:padding="15dip"/>
                <EditText
                    android:id="@+id/b_t"
                    android:layout_width="93dp"
                    android:background="@drawable/border"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:padding="15dip"/>
            </TableRow>
        </TableRow>
        <TableRow
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" >
            <TableRow
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" >
                <TextView
                    android:layout_width="92dp"
                    android:background="@drawable/border"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:padding="15dip"
                    android:text="C" />
                <EditText
                    android:id="@+id/c_r"
                    android:layout_width="93dp"
                    android:background="@drawable/border"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:padding="15dip" />
                <EditText
                    android:id="@+id/c_s"
                    android:layout_width="93dp"
                    android:background="@drawable/border"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:padding="15dip"/>
                <EditText
                    android:id="@+id/c_t"
                    android:layout_width="93dp"
                    android:background="@drawable/border"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:padding="15dip"/>
            </TableRow>
        </TableRow>
        <TableRow
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" >
            <TableRow
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" >
                <TextView
                    android:layout_width="92dp"
                    android:background="@drawable/border"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:padding="15dip"
                    android:text="D" />
                <EditText
                    android:id="@+id/d_r"
                    android:layout_width="93dp"
                    android:background="@drawable/border"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:padding="15dip" />
                <EditText
                    android:id="@+id/d_s"
                    android:layout_width="93dp"
                    android:background="@drawable/border"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:padding="15dip"/>
                <EditText
                    android:id="@+id/d_t"
                    android:layout_width="93dp"
                    android:background="@drawable/border"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:padding="15dip"/>
            </TableRow>
        </TableRow>
    </TableLayout>
    <TextView
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/header_form"
        android:textSize="@dimen/textsize"
        android:textColor="@color/White"
        android:text="LAIN LAIN" />
    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal" >

        <TextView
            android:layout_width="7dp"
            android:layout_height="wrap_content"
            android:layout_weight="0.25"
            android:text="Nomor Seri" />

        <EditText
            android:id="@+id/nomorSeri"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="0.75"
            android:ems="10" />
    </LinearLayout>
    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal" >

        <TextView
            android:layout_width="7dp"
            android:layout_height="wrap_content"
            android:layout_weight="0.25"
            android:text="Tanggal Pasang" />

        <EditText
            android:id="@+id/tanggalPasang"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="0.75"
            android:ems="10" />
    </LinearLayout>
    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal" >

        <TextView
            android:layout_width="7dp"
            android:layout_height="wrap_content"
            android:layout_weight="0.25"
            android:text="Tanggal Ganti" />

        <EditText
            android:id="@+id/tanggalGanti"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="0.75"
            android:ems="10" />
    </LinearLayout>
    <RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_gravity="center_vertical|center_horizontal">
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_centerHorizontal="true">
            <Button
                android:theme="@style/AppTheme.ButtonMenu"
                android:background="@drawable/button_save_gardu"
                android:id="@+id/backButton"
                android:layout_centerInParent="true"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="16dp"
                android:text=" &lt; BACK"
                android:textStyle="bold" />
            <Button
                android:theme="@style/AppTheme.ButtonMenu"
                android:background="@drawable/button_save_gardu"
                android:id="@+id/saveButton"
                android:layout_marginLeft="16dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="16dp"
                android:text="IMPAN"
                android:textStyle="bold" />
        </LinearLayout>
    </RelativeLayout>
</LinearLayout>
